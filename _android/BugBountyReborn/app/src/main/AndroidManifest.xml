<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="zack.bugbountyreborn" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_marshal"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:debuggable="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="zack.bugbountyreborn.MainActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/app_name"
            android:name="zack.bugbountyreborn.ProfileActivity"
            android:parentActivityName="zack.bugbountyreborn.MainActivity" >
        </activity>
        <activity
            android:label="@string/app_name"
            android:name="zack.bugbountyreborn.ReportActivity"
            android:parentActivityName="zack.bugbountyreborn.ProfileActivity" >
        </activity>
    </application>

</manifest>
